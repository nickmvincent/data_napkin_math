{
    "title": "Freelance Cost Per Book",
    "description": "Calculate the cost of commissioning a book based on freelance rates.",
    "inputs": [
      "average_length__book__words",
      "wage_data__generic_freelance_lower__dollars_per_word"
    ],
    "calculateType": "operations",
    "operations": [
      { "func": "multiply", "args": ["{average_length__book__words}", "{wage_data__generic_freelance_lower__dollars_per_word}"] }
    ],
    "result": {
      "label": "Cost Per Book",
      "units": "dollars"
    },
    "category": "Misc"
  },
  {
    "title": "Total Freelance Cost for Books",
    "description": "Estimate the total freelance cost for all books in the dataset.",
    "inputs": [
      "total_books__books3__books",
      "average_length__book__words",
      "wage_data__generic_freelance_lower__dollars_per_word"
    ],
    "calculateType": "operations",
    "operations": [
      { "func": "multiply", "args": ["{total_books__books3__books}", "{average_length__book__words}"], "name": "total_words" },
      { "func": "multiply", "args": ["{total_words}", "{wage_data__generic_freelance_lower__dollars_per_word}"] }
    ],
    "result": {
      "label": "Total Freelance Cost",
      "units": "dollars"
    },
    "category": "Misc"
  },
  {
    "title": "For reference: Revenue and tokens",
    "description": "Estimate the revenue generated per token in the dataset (divide {yearly_revenue__openai__dollars} by {dataset_size__llama3__tokens}).",
    "inputs": [
      "yearly_revenue__openai__dollars",
      "dataset_size__llama3__tokens"
    ],
    "calculateType": "operations",
    "operations": [
      { "func": "divide", "args": ["{yearly_revenue__openai__dollars}", "{dataset_size__llama3__tokens}"] }
    ],
    "result": {
      "label": "Revenue Per Token",
      "units": "dollars"
    },
    "category": "Misc"
  },
  {
    "title": "For reference: Number of Contributions in a 'Dataset'",
    "description": "Calculate the number of distinct contributions based on tokens.",
    "inputs": [
      "dataset_size__llama3__tokens",
      "dataset_attribute__redpajama__tokens_per_contribution"
    ],
    "calculateType": "operations",
    "operations": [
      { "func": "divide", "args": ["{dataset_size__llama3__tokens}", "{dataset_attribute__redpajama__tokens_per_contribution}"] }
    ],
    "result": {
      "label": "Contributions",
      "units": "documents"
    },
    "category": "Misc"
  },
  {
    "title": "For reference: Revenue Per Contribution",
    "description": "Estimate the revenue generated per contribution based on tokens.",
    "inputs": [
      "yearly_revenue__openai__dollars",
      "dataset_size__llama3__tokens",
      "dataset_attribute__redpajama__tokens_per_contribution"
    ],
    "calculateType": "operations",
    "operations": [
      { "func": "divide", "args": ["{yearly_revenue__openai__dollars}", "{dataset_size__llama3__tokens}"], "name": "revenue_per_token" },
      { "func": "multiply", "args": ["{revenue_per_token}", "{dataset_attribute__redpajama__tokens_per_contribution}"] }
    ],
    "result": {
      "label": "Revenue Per Contribution",
      "units": "dollars"
    },
    "category": "Misc"
  },
  {
    "title": "For reference: Revenue Per Book",
    "description": "Estimate the revenue generated per book in a dataset. Assume that each book's share is proportionate to its overall token contribution.",
    "inputs": [
      "yearly_revenue__openai__dollars",
      "dataset_size__llama3__tokens",
      "training_detail__openai__words_per_token",
      "average_length__book__words"
    ],
    "calculateType": "operations",
    "operations": [
      { "func": "divide", "args": ["{average_length__book__words}", "{training_detail__openai__words_per_token}"], "name": "tokens_per_book" },
      { "func": "divide", "args": ["{tokens_per_book}", "{dataset_size__llama3__tokens}"], "name": "proportion_of_dataset" },
      { "func": "multiply", "args": ["{proportion_of_dataset}", "{yearly_revenue__openai__dollars}"] }
    ],
    "result": {
      "label": "Revenue Per Book",
      "units": "dollars"
    },
    "category": "Misc"
  },
  {
